function day1(){
   setTimeout(()=>{
      console.log("exam1")   
   },Math.random()*1000)

}

function day2(){
   setTimeout(()=>{
      console.log("exam2")   
   },Math.random()*2000)

}
function day3(){
   setTimeout(()=>{
      console.log("exam3")   
   },Math.random()*500)

}
day1();
day2();
day3();
